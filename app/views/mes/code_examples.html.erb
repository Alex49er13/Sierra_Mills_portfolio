puts &quot;enter a number&quot;<br/>
number = gets.chomp.to_i<br/>

def right_most_number(number)<br/>

if number ==13 ||number ==11 || number == 12<br/>
return 'th'<br/>
end <br/>

number = number % 10<br/>


if number == 1<br/>
return 'st'<br/>

elsif number == 2<br/>
return 'nd'<br/>

elsif number == 3<br/>
return 'rd'<br/>

else<br/>
return 'th'<br/>

end<br/>
end<br/>

puts &quot;Thats the #{number}#{right_most_number(number)} item!&quot;<br/>
puts &quot;goodbye&quot;<br/>



